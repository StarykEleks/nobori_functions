{
  "name": "@nobori/functions-express",
  "private": true,
  "main": "dist/index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "gcp-build": "echo 'skip build in Cloud Build'",
    "start:dev": "functions-framework --source=./dist/index --target=visabilityCheck --port=8081",
    "start:ts": "ts-node --transpile-only src/devServer.ts",
    "start:http": "yarn start:dev --signature-type=http",
    "start:visabilityCheck": "yarn start:http --target=visabilityCheck --source=./src/visabilityCheck"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^4.0.0",
    "@google-cloud/pubsub": "^5.2.0",
    "@google-cloud/tasks": "^6.2.0",
    "@types/express": "^5.0.3",
    "@types/pg": "^8.15.5",
    "cloudevents": "^10.0.0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "firebase-admin": "^13.5.0",
    "google-auth-library": "^10.4.1",
    "google-gax": "^5.0.4",
    "ioredis": "^5.8.2",
    "js-yaml": "^4.1.0",
    "openai": "^6.6.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
